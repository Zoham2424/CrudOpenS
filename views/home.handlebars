<h1>Employee list 
  
</h1>
<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Department</th>
            <th>Start Date</th>
            <th>Job Title</th>
            <th>Salary</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {{#each employees}}
        <tr>
            <th>{{employees.firstName}}</th>
            <th>{{employees.lastName}}</th>
            <th>{{employees.department}}</th>
            <th>{{employees.startDate}}</th>
            <th>{{employees.jobTitle}}</th>
            <th>{{employees.salary}}</th>
            <th>
                <a href="/updateEmployee/{{employees._id}}">Update</a>
                <form action="/deleteEmployee/{{employees._id}}" method="POST" style="display:inline;">
                    <button type="submit">Delete</button>
                </form>
            </th>
        </tr>
        {{/each}}
    </tbody>
</table>